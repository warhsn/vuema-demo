{
  "name": "vue-bulma-4",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "deploy": "npm run build && cd dist && git init && git add -A && git commit -m 'deploy' && git push -f git@github.com:warhsn/https://warhsn.github.io/vuema-demo.git main:gh-pages"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@warhsn/vuema": "^0.6.6",
    "bulma": "^1.0.0",
    "dart-sass": "^1.25.0",
    "highlight-vue": "^1.1.6",
    "highlight.js": "^11.9.0",
    "pinia": "^2.0.23",
    "sass": "^1.67.0",
    "vue": "^3.4.29",
    "vue-github-button": "^3.1.3",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "unplugin-vue-import-props": "^0.0.7-beta.2",
    "vite": "^3.1.8",
    "vite-plugin-vue-type-imports": "^0.2.4",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.8.27"
  }
}
